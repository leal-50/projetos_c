                printf("1 - População\n");
                printf("2 - Área\n");
                printf("3 - PIB\n");
                printf("4 - Pontos Turísticos\n");
                printf("Digite sua escolha: ");
                scanf("%d", &escolha2);
                printf("\n");

                switch (escolha2) {
                    case 1:  // Lógica para comparar a população
                        printf("Atributo: População\n");
                    if (densidade2 > densidade1 && populacao1 > populacao2) {
                        printf("%s - %s\n", nome_cid1, nome_cid2);
                        printf("Os atributos usados foram: Densidade Demográfica e População\n");
                        printf("Densidade Demográfica da Carta %s: %.2f - Densidade Demográfica da Carta %s: %.2f\n", nome_cid1, densidade1, nome_cid2, densidade2);
                        printf("População da Carta %s: %d habitantes - População da Carta %s: %d habitantes\n", nome_cid1, populacao1, nome_cid2, populacao2);
                        printf("A soma da Carta %s deu: %.2f - A soma da Carta %s deu: %.2f\n", nome_cid1, (densidade1 + populacao1), nome_cid2, (densidade2 + populacao2));
                        printf("Venceu a %s\n", nome_cid1);
                    }
                    else if (populacao2 > populacao1 && densidade1 > densidade2) {
                        printf("%s - %s\n", nome_cid1, nome_cid2);
                        printf("Os atributos usados foram: Densidade Demográfica e População\n");
                        printf("Densidade Demográfica da Carta %s: %.2f - Densidade Demográfica da Carta %s: %.2f\n", nome_cid1, densidade1, nome_cid2, densidade2);
                        printf("População da Carta %s: %d habitantes - População da Carta %s: %d habitantes\n", nome_cid1, populacao1, nome_cid2, populacao2);
                        printf("A soma da Carta %s deu: %.2f - A soma da Carta %s deu: %.2f\n", nome_cid1, (densidade1 + populacao1), nome_cid2, (densidade2 + populacao2));
                        printf("Venceu a %s\n", nome_cid2);
                    }
                    else {
                        printf("%s - %s\n", nome_cid1, nome_cid2);
                        printf("Os atributos usados foram: Densidade Demográfica e População\n");
                        printf("Densidade Demográfica da Carta %s: %.2f - Densidade Demográfica da Carta %s: %.2f\n", nome_cid1, densidade1, nome_cid2, densidade2);
                        printf("População da Carta %s: %d habitantes - População da Carta %s: %d habitantes\n", nome_cid1, populacao1, nome_cid2, populacao2);
                        printf("A soma da Carta %s deu: %.2f - A soma da Carta %s deu: %.2f\n", nome_cid1, (densidade1 + populacao1), nome_cid2, (densidade2 + populacao2));
                        printf("Houve Empate\n");
                    }
                break;
                case 2: // Lógica para comparar a área
                        printf("Atributo: Área\n");
                    if (densidade2 > densidade1 && area1 > area2) {
                        printf("%s - %s\n", nome_cid1, nome_cid2);
                        printf("Os atributos usados foram: Densidade Demográfica e Área\n");
                        printf("Densidade Demográfica da Carta %s: %.2f - Densidade Demográfica da Carta %s: %.2f\n", nome_cid1, densidade1, nome_cid2, densidade2);
                        printf("Área da Carta %s: %.2f km² - Área da Carta %s: %.2f km²\n", nome_cid1, area1, nome_cid2, area2);
                        printf("A soma da Carta %s deu: %.2f - A soma da Carta %s deu: %.2f\n", nome_cid1, (densidade1 + area1), nome_cid2, (densidade2 + area2));
                        printf("Venceu a %s\n", nome_cid1);
                    }
                    else if (area2 > area1 && densidade1 > densidade2) {
                        printf("%s - %s\n", nome_cid1, nome_cid2);
                        printf("Os atributos usados foram: Densidade Demográfica e Área\n");
                        printf("Densidade Demográfica da Carta %s: %.2f - Densidade Demográfica da Carta %s: %.2f\n", nome_cid1, densidade1, nome_cid2, densidade2);
                        printf("Área da Carta %s: %.2f km² - Área da Carta %s: %.2f km²\n", nome_cid1, area1, nome_cid2, area2);
                        printf("A soma da Carta %s deu: %.2f - A soma da Carta %s deu: %.2f\n", nome_cid1, (densidade1 + area1), nome_cid2, (densidade2 + area2));
                        printf("Venceu a %s\n", nome_cid2);
                    }
                    else {
                        printf("%s - %s\n", nome_cid1, nome_cid2);
                        printf("Os atributos usados foram: Densidade Demográfica e Área\n");
                        printf("Densidade Demográfica da Carta %s: %.2f - Densidade Demográfica da Carta %s: %.2f\n", nome_cid1, densidade1, nome_cid2, densidade2);
                        printf("Área da Carta %s: %.2f km² - Área da Carta %s: %.2f km²\n", nome_cid1, area1, nome_cid2, area2);
                        printf("A soma da Carta %s deu: %.2f - A soma da Carta %s deu: %.2f\n", nome_cid1, (densidade1 + area1), nome_cid2, (densidade2 + area2));
                        printf("Houve Empate\n");
                    }
                break;
                case 3: // Lógica para comparar o PIB
                        printf("Atributo: PIB\n");
                    if (densidade2 > densidade1 && pib1 > pib2) {
                        printf("%s - %s\n", nome_cid1, nome_cid2);
                        printf("Os atributos usados foram: Densidade Demográfica e PIB\n");
                        printf("Densidade Demográfica da Carta %s: %.2f - Densidade Demográfica da Carta %s: %.2f\n", nome_cid1, densidade1, nome_cid2, densidade2);
                        printf("PIB da Carta %s: %.2f - PIB da Carta %s: %.2f\n", nome_cid1, pib1, nome_cid2, pib2);
                        printf("A soma da Carta %s deu: %.2f - A soma da Carta %s deu: %.2f\n", nome_cid1, (densidade1 + pib1), nome_cid2, (densidade2 + pib2));
                        printf("Venceu a %s\n", nome_cid1);
                    }
                    else if (pib2 > pib1 && densidade1 > densidade2) {
                        printf("%s - %s\n", nome_cid1, nome_cid2);
                        printf("Os atributos usados foram: Densidade Demográfica e PIB\n");
                        printf("Densidade Demográfica da Carta %s: %.2f - Densidade Demográfica da Carta %s: %.2f\n", nome_cid1, densidade1, nome_cid2, densidade2);
                        printf("PIB da Carta %s: %.2f - PIB da Carta %s: %.2f\n", nome_cid1, pib1, nome_cid2, pib2);
                        printf("A soma da Carta %s deu: %.2f - A soma da Carta %s deu: %.2f\n", nome_cid1, (densidade1 + pib1), nome_cid2, (densidade2 + pib2));
                        printf("Venceu a %s\n", nome_cid2);
                    }
                    else {
                        printf("%s - %s\n", nome_cid1, nome_cid2);
                        printf("Os atributos usados foram: Densidade Demográfica e PIB\n");
                        printf("Densidade Demográfica da Carta %s: %.2f - Densidade Demográficada Carta %s: %.2f\n", nome_cid1, densidade1, nome_cid2, densidade2);
                        printf("PIB da Carta %s: %.2f - PIB da Carta %s: %.2f\n", nome_cid1, pib1, nome_cid2, pib2);
                        printf("A soma da Carta %s deu: %.2f - A soma da Carta %s deu: %.2f\n", nome_cid1, (densidade1 + pib1), nome_cid2, (densidade2 + pib2));
                        printf("Houve Empate\n");
                    }
                break;
                case 4: // Lógica para comparar o número de pontos turísticos
                        printf("Atributo: Pontos Turísticos\n");
                    if (densidade2 > densidade1 && num_pt1 > num_pt2) {
                        printf("%s - %s\n", nome_cid1, nome_cid2);
                        printf("Os atributos usados foram: Densidade Demográfica e Pontos Turísticos\n");
                        printf("Densidade Demográfica da Carta %s: %.2f - Densidade Demográfica da Carta %s: %.2f\n", nome_cid1, densidade1, nome_cid2, densidade2);
                        printf("Pontos Turísticos da Carta %s: %d - Pontos Turísticos da Carta %s: %d\n", nome_cid1, num_pt1, nome_cid2, num_pt2);
                        printf("A soma da Carta %s deu: %.2f - A soma da Carta %s deu: %.2f\n", nome_cid1, (densidade1 + num_pt1), nome_cid2, (densidade2 + num_pt2));
                        printf("Venceu a %s\n", nome_cid1);
                    }
                    else if (num_pt2 > num_pt1 && densidade1 > densidade2) {
                        printf("%s - %s\n", nome_cid1, nome_cid2);
                        printf("Os atributos usados foram: Densidade Demográfica e Pontos Turísticos\n");
                        printf("Densidade Demográfica da Carta %s: %.2f - Densidade Demográfica da Carta %s: %.2f\n", nome_cid1, densidade1, nome_cid2, densidade2);
                        printf("Pontos Turísticos da Carta %s: %d - Pontos Turísticos da Carta %s: %d\n", nome_cid1, num_pt1, nome_cid2, num_pt2);
                        printf("A soma da Carta %s deu: %.2f - A soma da Carta %s deu: %.2f\n", nome_cid1, (densidade1 + num_pt1), nome_cid2, (densidade2 + num_pt2));
                        printf("Venceu a %s\n", nome_cid2);
                    }
                    else {
                        printf("%s - %s\n", nome_cid1, nome_cid2);
                        printf("Os atributos usados foram: Densidade Demográfica e Pontos Turísticos\n");
                        printf("Densidade Demográfica da Carta %s: %.2f - Densidade Demográfica da Carta %s: %.2f\n", nome_cid1, densidade1, nome_cid2, densidade2);
                        printf("Pontos Turísticos da Carta %s: %d - Pontos Turísticos da Carta %s: %d\n", nome_cid1, num_pt1, nome_cid2, num_pt2);
                        printf("A soma da Carta %s deu: %.2f - A soma da Carta %s deu: %.2f\n", nome_cid1, (densidade1 + num_pt1), nome_cid2, (densidade2 + num_pt2));
                        printf("Houve Empate\n");
                    }
                break;
                default: // Caso o jogador escolha uma opção inválida
                    printf("Opção Inválida\n");
                break;
                }
        break;
        default: // Caso o jogador escolha uma opção inválida
            printf("Opção Inválida\n");
        break;
        }

    return 0;
}
// Fim do código
// O código acima é um jogo de cartas onde o jogador pode comparar 2 tipos de atributos de cidades.

