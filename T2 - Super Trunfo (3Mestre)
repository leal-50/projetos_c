#include <stdio.h>
#include <stdlib.h>
#include <time.h>

int main() {

    // Variáveis da 1° Cidade:
    char estado1[3]; // Vai armazenar a letra do estado (array de 3 caractere)
    char cod_carta1[5]; // Vai armazenar o código da carta (array de 4 caracteres, incluindo o terminador nulo)
    char nome_cid1[20]; // Vai armazenar o nome da cidade (array de 20 caracteres)
    int populacao1; // Vai armazenar o número da população (números sem casas decimais)
    float area1; // Vai armazenar o tamanho da área da cidade (números com casas decimais)
    float pib1; // Vai armazenar a quantidade do PIB da cidade (números com casas decimais)
    int num_pt1; // Vai armazenar o número de pontos turísticos (números sem casas decimais)
    float densidade1; // Vai armazenar a densidade populacional da cidade (números com casas decimais)
    float pib_per_capita1; // Vai armazenar o PIB per capita da cidade (números com casas decimais)

    // Recebendo dados da 1° Cidade
    printf("Dados da 1° Carta:\n");
    printf("Digite a sigla do estado: ");
    scanf("%s", estado1);
    printf("Digite a letra do estado seguida de um número de 01 a 04 (ex: A01, B03): ");
    scanf("%s", cod_carta1);
    printf("Digite o nome da cidade: ");
    scanf("%s", nome_cid1);
    printf("Digite o número de habitantes da cidade: ");
    scanf("%d", &populacao1);
    printf("Digite a área da cidade em quilômetros quadrados: ");
    scanf("%f", &area1);
    printf("Digite o Produto Interno Bruto da cidade: ");
    scanf("%f", &pib1);
    printf("Digite a quantidade de pontos turísticos na cidade: ");
    scanf("%d", &num_pt1);
    printf("\n");

    // Calculando a densidade demográfica e o PIB per capita da 1° cidade
    densidade1 = (float) populacao1 / area1; // "populacao" e "area" são convertidos para float
    pib_per_capita1 = (float) pib1 / populacao1; // Cálculo do PIB per capita

    // Variáveis da 2° Cidade:
    char estado2[3]; // Vai armazenar a letra do estado (array de 1 caractere)
    char cod_carta2[5]; // Vai armazenar o código da carta (array de 4 caracteres, incluindo o terminador nulo)
    char nome_cid2[20]; // Vai armazenar o nome da cidade (array de 20 caracteres)
    int populacao2; // Vai armazenar o número da população (números sem casas decimais)
    float area2; // Vai armazenar o tamanho da área da cidade (números com casas decimais)
    float pib2; // Vai armazenar a quantidade do PIB da cidade (números com casas decimais)
    int num_pt2; // Vai armazenar o número de pontos turísticos (números sem casas decimais)
    float densidade2; // Vai armazenar a densidade populacional da cidade (números com casas decimais)
    float pib_per_capita2; // Vai armazenar o PIB per capita da cidade (números com casas decimais)

    // Recebendo dados da 2° Cidade
    printf("Dados da 2° Carta:\n");
    printf("Digite a sigla do estado: ");
    scanf(" %s", &estado2);
    printf("Digite a letra do estado seguida de um número de 01 a 04 (ex: A01, B03): ");
    scanf("%s", cod_carta2);
    printf("Digite o nome da cidade: ");
    scanf("%s", nome_cid2);
    printf("Digite o número de habitantes da cidade: ");
    scanf("%d", &populacao2);
    printf("Digite a área da cidade em quilômetros quadrados: ");
    scanf("%f", &area2);
    printf("Digite o Produto Interno Bruto da cidade: ");
    scanf("%f", &pib2);
    printf("Digite a quantidade de pontos turísticos na cidade: ");
    scanf("%d", &num_pt2);
    printf("\n");

    // Calculando a densidade demográfica e o PIB per capita da 2° cidade
    densidade2 = (float) populacao2 / area2; // "populacao" e "area" são convertidos para float
    pib_per_capita2 = (float) pib2 / populacao2; // Cálculo do PIB per capita


    // O jogador escolhe um atributo para comparar as cartas
    int escolha1, escolha2; // Variáveis para armazenar a escolha do jogador

    printf("\n");
    printf("Comparador de Cartas\n");
    printf("Escolha um atributo para comparar:\n");
    printf("1 - População\n");
    printf("2 - Área\n");
    printf("3 - PIB\n");
    printf("4 - Pontos Turísticos\n");
    printf("5 - Densidade Demográfica\n");
    printf("Digite sua escolha: ");
    scanf("%d", &escolha1);
    printf("\n");

    switch (escolha1) {
        case 1: // Lógica para comparar a população
            printf("Atributo: População\n");

                printf("\n");
                printf("Escolha outro atributo para comparar:\n");
                printf("1 - Área\n");
                printf("2 - PIB\n");
                printf("3 - Pontos Turísticos\n");
                printf("4 - Densidade Demográfica\n");
                printf("Digite sua escolha: ");
                scanf("%d", &escolha2);
                printf("\n");

                switch (escolha2) {
                    case 1:  // Lógica para comparar a área
                            printf("Atributo: Área\n");
                        if (populacao1 > populacao2 && area1 > area2) {
                            printf("%s - %s\n", nome_cid1, nome_cid2);
                            printf("Os atributos usados foram: População e Área\n");
                            printf("População da Carta %s: %d habitantes - População da Carta %s: %d habitantes\n", nome_cid1, populacao1, nome_cid2, populacao2);
                            printf("Área da Carta %s: %.2f km² - Área da Carta %s: %.2f km²\n", nome_cid1, area1, nome_cid2, area2);
                            printf("A soma da Carta %s deu: %.2f - A soma da Carta %s deu: %.2f\n", nome_cid1, (populacao1 + area1), nome_cid2, (populacao2 + area2));
                            printf("Venceu a %s\n", nome_cid1);
                        }
                        else if (area2 > area1 && populacao2 > populacao1) {
                            printf("%s - %s\n", nome_cid1, nome_cid2);
                            printf("Os atributos usados foram: População e Área\n");
                            printf("População da Carta %s: %d habitantes - População da Carta %s: %d habitantes\n", nome_cid1, populacao1, nome_cid2, populacao2);
                            printf("Área da Carta %s: %.2f km² - Área da Carta %s: %.2f km²\n", nome_cid1, area1, nome_cid2, area2);
                            printf("A soma da Carta %s deu: %.2f - A soma da Carta %s deu: %.2f\n", nome_cid1, (populacao1 + area1), nome_cid2, (populacao2 + area2));
                            printf("Venceu a %s\n", nome_cid2);
                        }
                        else {
                            printf("%s - %s\n", nome_cid1, nome_cid2);
                            printf("Os atributos usados foram: População e Área\n");
                            printf("População da Carta %s: %d habitantes - População da Carta %s: %d habitantes\n", nome_cid1, populacao1, nome_cid2, populacao2);
                            printf("Área da Carta %s: %.2f km² - Área da Carta %s: %.2f km²\n", nome_cid1, area1, nome_cid2, area2);
                            printf("A soma da Carta %s deu: %.2f - A soma da Carta %s deu: %.2f\n", nome_cid1, (populacao1 + area1), nome_cid2, (populacao2 + area2));
                            printf("Houve Empate\n");
                        }
                    break;

                    case 2: // Lógica para comparar o PIB
                            printf("Atributo: PIB\n");
                        if (populacao1 > populacao2 && pib1 > pib2) {
                            printf("%s - %s\n", nome_cid1, nome_cid2);
                            printf("Os atributos usados foram: População e PIB\n");
                            printf("População da Carta %s: %d habitantes - População da Carta %s: %d habitantes\n", nome_cid1, populacao1, nome_cid2, populacao2);
                            printf("PIB da Carta %s: %.2f - PIB da Carta %s: %.2f\n", nome_cid1, pib1, nome_cid2, pib2);
                            printf("A soma da Carta %s deu: %.2f - A soma da Carta %s deu: %.2f\n", nome_cid1, (populacao1 + pib1), nome_cid2, (populacao2 + pib2));
                            printf("Venceu a %s\n", nome_cid1);
                        }
                        else if (pib2 > pib1 && populacao2 > populacao1) {
                            printf("%s - %s\n", nome_cid1, nome_cid2);
                            printf("Os atributos usados foram: População e PIB\n");
                            printf("População da Carta %s: %d habitantes - População da Carta %s: %d habitantes\n", nome_cid1, populacao1, nome_cid2, populacao2);
                            printf("PIB da Carta %s: %.2f - PIB da Carta %s: %.2f\n", nome_cid1, pib1, nome_cid2, pib2);
                            printf("A soma da Carta %s deu: %.2f - A soma da Carta %s deu: %.2f\n", nome_cid1, (populacao1 + pib1), nome_cid2, (populacao2 + pib2));
                            printf("Venceu a %s\n", nome_cid2);
                        }
                        else {
                            printf("%s - %s\n", nome_cid1, nome_cid2);
                            printf("Os atributos usados foram: População e PIB\n");
                            printf("População da Carta %s: %d habitantes - População da Carta %s: %d habitantes\n", nome_cid1, populacao1, nome_cid2, populacao2);
                            printf("PIB da Carta %s: %.2f - PIB da Carta %s: %.2f\n", nome_cid1, pib1, nome_cid2, pib2);
                            printf("A soma da Carta %s deu: %.2f - A soma da Carta %s deu: %.2f\n", nome_cid1, (populacao1 + pib1), nome_cid2, (populacao2 + pib2));
                            printf("Houve Empate\n");
                        }
                    break;

                    case 3: // Lógica para comparar o número de pontos turísticos
                            printf("Atributo: Pontos Turísticos\n");
                        if (populacao1 > populacao2 && num_pt1 > num_pt2) {
                            printf("%s - %s\n", nome_cid1, nome_cid2);
                            printf("Os atributos usados foram: População e Pontos Turísticos\n");
                            printf("População da Carta %s: %d habitantes - População da Carta %s: %d habitantes\n", nome_cid1, populacao1, nome_cid2, populacao2);
                            printf("Pontos Turísticos da Carta %s: %d - Pontos Turísticos da Carta %s: %d\n", nome_cid1, num_pt1, nome_cid2, num_pt2);
                            printf("A soma da Carta %s deu: %.2f - A soma da Carta %s deu: %.2f\n", nome_cid1, (populacao1 + num_pt1), nome_cid2, (populacao2 + num_pt2));
                            printf("Venceu a %s\n", nome_cid1);
                        }
                        else if (num_pt2 > num_pt1 && populacao2 > populacao1) {
                            printf("%s - %s\n", nome_cid1, nome_cid2);
                            printf("Os atributos usados foram: População e Pontos Turísticos\n");
                            printf("População da Carta %s: %d habitantes - População da Carta %s: %d habitantes\n", nome_cid1, populacao1, nome_cid2, populacao2);
                            printf("Pontos Turísticos da Carta %s: %d - Pontos Turísticos da Carta %s: %d\n", nome_cid1, num_pt1, nome_cid2, num_pt2);
                            printf("A soma da Carta %s deu: %.2f - A soma da Carta %s deu: %.2f\n", nome_cid1, (populacao1 + num_pt1), nome_cid2, (populacao2 + num_pt2));
                            printf("Venceu a %s\n", nome_cid2);
                        }
                        else {
                            printf("%s - %s\n", nome_cid1, nome_cid2);
                            printf("Os atributos usados foram: População e Pontos Turísticos\n");
                            printf("População da Carta %s: %d habitantes - População da Carta %s: %d habitantes\n", nome_cid1, populacao1, nome_cid2, populacao2);
                            printf("Pontos Turísticos da Carta %s: %d - Pontos Turísticos da Carta %s: %d\n", nome_cid1, num_pt1, nome_cid2, num_pt2);
                            printf("A soma da Carta %s deu: %.2f - A soma da Carta %s deu: %.2f\n", nome_cid1, (populacao1 + num_pt1), nome_cid2, (populacao2 + num_pt2));
                            printf("Houve Empate\n");
                        }
                    break;
                    case 4: // Lógica para comparar a densidade demográfica
                            printf("Atributo: Densidade Demográfica\n");
                        if (populacao1 > populacao2 && densidade2 > densidade1) {
                            printf("%s - %s\n", nome_cid1, nome_cid2);
                            printf("Os atributos usados foram: População e Densidade Demográfica\n");
                            printf("População da Carta %s: %d habitantes - População da Carta %s: %d habitantes\n", nome_cid1, populacao1, nome_cid2, populacao2);
                            printf("Densidade Demográfica da Carta %s: %.2f - Densidade Demográfica da Carta %s: %.2f\n", nome_cid1, densidade1, nome_cid2, densidade2);
                            printf("A soma da Carta %s deu: %.2f - A soma da Carta %s deu: %.2f\n", nome_cid1,(populacao1 + densidade1), nome_cid2, (populacao2 + densidade2));
                            printf("Venceu a %s\n", nome_cid1);
                        }
                        else if (densidade1 > densidade2 && populacao2 > populacao1) {
                            printf("%s - %s\n", nome_cid1, nome_cid2);
                            printf("Os atributos usados foram: População e Densidade Demográfica\n");
                            printf("População da Carta %s: %d habitantes - População da Carta %s: %d habitantes\n", nome_cid1, populacao1, nome_cid2, populacao2);
                            printf("Densidade Demográfica da Carta %s: %.2f - Densidade Demográfica da Carta %s: %.2f\n", nome_cid1, densidade1, nome_cid2, densidade2);
                            printf("A soma da Carta %s deu: %.2f - A soma da Carta %s deu: %.2f\n", nome_cid1, (populacao1 + densidade1), nome_cid2, (populacao2 + densidade2));
                            printf("Venceu a %s\n", nome_cid2);
                        }
                        else {
                            printf("%s - %s\n", nome_cid1, nome_cid2);
                            printf("Os atributos usados foram: População e Densidade Demográfica\n");
                            printf("População da Carta %s: %d habitantes - População da Carta %s: %d habitantes\n", nome_cid1, populacao1, nome_cid2, populacao2);
                            printf("Densidade Demográfica da Carta %s: %.2f - Densidade Demográfica da Carta %s: %.2f\n", nome_cid1, densidade1, nome_cid2, densidade2);
                            printf("A soma da Carta %s deu: %.2f - A soma da Carta %s deu: %.2f\n", nome_cid1, (populacao1 + densidade1), nome_cid2, (populacao2 + densidade2));
                            printf("Houve Empate\n");
                        }
                    break;

                    default: // Caso o jogador escolha uma opção inválida
                        printf("Opção Inválida\n");
                    break;
                    }
        break;

        case 2: // Lógica para comparar a área
            printf("Atributo: Área\n");
            
                printf("\n");
                printf("Escolha outro atributo para comparar:\n");
                printf("1 - População\n");
                printf("2 - PIB\n");
                printf("3 - Pontos Turísticos\n");
                printf("4 - Densidade Demográfica\n");
                printf("Digite sua escolha: ");
                scanf("%d", &escolha2);
                printf("\n");

                switch (escolha2) {
                    case 1:  // Lógica para comparar a população
                        printf("Atributo: População\n");
                    if (area1 > area2 && populacao1 > populacao2) {
                        printf("%s - %s\n", nome_cid1, nome_cid2);
                        printf("Os atributos usados foram: Área e População\n");
                        printf("Área da Carta %s: %.2f km² - Área da Carta %s: %.2f km²\n", nome_cid1, area1, nome_cid2, area2);
                        printf("População da Carta %s: %d habitantes - População da Carta %s: %d habitantes\n", nome_cid1, populacao1, nome_cid2, populacao2);
                        printf("A soma da Carta %s deu: %.2f - A soma da Carta %s deu: %.2f\n", nome_cid1, (area1 + populacao1), nome_cid2, (area2 + populacao2));
                        printf("Venceu a %s\n", nome_cid1);
                    }
                    else if (populacao2 > populacao1 && area2 > area1) {
                        printf("%s - %s\n", nome_cid1, nome_cid2);
                        printf("Os atributos usados foram: Área e População\n");
                        printf("Área da Carta %s: %.2f km² - Área da Carta %s: %.2f km²\n", nome_cid1, area1, nome_cid2, area2);
                        printf("População da Carta %s: %d habitantes - População da Carta %s: %d habitantes\n", nome_cid1, populacao1, nome_cid2, populacao2);
                        printf("A soma da Carta %s deu: %.2f - A soma da Carta %s deu: %.2f\n", nome_cid1, (area1 + populacao1), nome_cid2, (area2 + populacao2));
                        printf("Venceu a %s\n", nome_cid2);
                    }
                    else {
                        printf("%s - %s\n", nome_cid1, nome_cid2);
                        printf("Os atributos usados foram: Área e População\n");
                        printf("Área da Carta %s: %.2f km² - Área da Carta %s: %.2f km²\n", nome_cid1, area1, nome_cid2, area2);
                        printf("População da Carta %s: %d habitantes - População da Carta %s: %d habitantes\n", nome_cid1, populacao1, nome_cid2, populacao2);
                        printf("A soma da Carta %s deu: %.2f - A soma da Carta %s deu: %.2f\n", nome_cid1, (area1 + populacao1), nome_cid2, (area2 + populacao2));
                        printf("Houve Empate\n");
                    }
                break;

                case 2: // Lógica para comparar o PIB
                        printf("Atributo: PIB\n");
                    if (area1 > area2 && pib1 > pib2) {
                        printf("%s - %s\n", nome_cid1, nome_cid2);
                        printf("Os atributos usados foram: Área e PIB\n");
                        printf("Área da Carta %s: %.2f km² - Área da Carta %s: %.2f km²\n", nome_cid1, area1, nome_cid2, area2);
                        printf("PIB da Carta %s: %.2f - PIB da Carta %s: %.2f\n", nome_cid1, pib1, nome_cid2, pib2);
                        printf("A soma da Carta %s deu: %.2f - A soma da Carta %s deu: %.2f\n", nome_cid1, (area1 + pib1), nome_cid2, (area2 + pib2));
                        printf("Venceu a %s\n", nome_cid1);
                    }
                    else if (pib2 > pib1 && area2 > area1) {
